package com.sonata.RegistrationPackage;

public class RegistrationBean extends Address{
	
	public int custId;
	public String name;
	public String password;
	public String username;
	public int mobileno;
	public  int alternateMobNo;
	/*public Address emailId;
	public Address country;
	public Address state;
	public Address city;
	public Address street;
	public Address pinCode;*/
	public RegistrationBean(int custId, String name, String password, String username, int mobileno,
			int alternateMobNo) {
		super();
		this.custId = custId;
		this.name = name;
		this.password = password;
		this.username = username;
		this.mobileno = mobileno;
		this.alternateMobNo = alternateMobNo;
	}
	
	
	
	
}
