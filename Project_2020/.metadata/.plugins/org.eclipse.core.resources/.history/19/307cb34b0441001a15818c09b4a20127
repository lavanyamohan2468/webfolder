package com.sonata.RegistrationPackage;

public class RegistrationBean extends Address{
	
	public int custId;
	public String name;
	public String password;
	public String username;
	public int mobileno;
	public  int alternateMobNo;
	private int groupId;
	/*public Address emailId;
	public Address country;
	public Address state;
	public Address city;
	public Address street;
	public Address pinCode;*/
	
	RegistrationBean(){}
	
	public RegistrationBean(int custId, String name, String password, String username, int mobileno,
			int alternateMobNo) {
		super();
		this.custId = custId;
		this.name = name;
		this.password = password;
		this.username = username;
		this.mobileno = mobileno;
		this.alternateMobNo = alternateMobNo;
	}
	
	
	
	public RegistrationBean( String username,String password) {
		super();
		this.password = password;
		this.username = username;
	}

	public int getCustId() {
		return custId;
	}
	public void setCustId(int custId) {
		this.custId = custId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public int getMobileno() {
		return mobileno;
	}
	public void setMobileno(int mobileno) {
		this.mobileno = mobileno;
	}
	@Override
	public String toString() {
		return "RegistrationBean [custId=" + custId + ", name=" + name + ", password=" + password + ", username="
				+ username + ", mobileno=" + mobileno + ", alternateMobNo=" + alternateMobNo + "]";
	}
	
	
	
	
}
